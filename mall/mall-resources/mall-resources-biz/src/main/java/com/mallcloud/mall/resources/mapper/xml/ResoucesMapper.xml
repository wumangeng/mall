<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mallcloud.mall.resources.mapper.ResoucesMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mallcloud.mall.resources.entity.Resources">
        <id column="id" property="id" />
        <result column="file_name" property="fileName" />
        <result column="scene" property="scene" />
        <result column="url" property="url" />
        <result column="path" property="path" />
        <result column="src" property="src" />
        <result column="domain" property="domain" />
        <result column="status" property="status" />
        <result column="md5" property="md5" />
        <result column="size" property="size" />
        <result column="mtime" property="mtime" />
        <result column="retmsg" property="retmsg" />
        <result column="message" property="message" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, file_name, scene, url, path, src, domain, status, md5, size, mtime, retmsg, message
    </sql>

</mapper>
